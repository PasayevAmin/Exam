@using Indigo_Exam.Areas.Manage.ViewModels;
@model UpdateSettingVM

<div class="card-body">
    <h4 class="card-title">Basic form elements</h4>
    <p class="card-description">
        Basic form elements
    </p>
    <form class="forms-sample" method="post" enctype="multipart/form-data">
        <div class="form-group">
            <label asp-for="Key">Key</label>
            <input asp-for="Key" type="text" class="form-control" id="exampleInputName1" placeholder="Key">
            <span asp-validation-for="Key"></span>
        </div>
        <div class="form-group">
            <label asp-for="Value">Value</label>
            <input asp-for="Value" type="text" class="form-control" id="exampleInputName1" placeholder="Value">
            <span asp-validation-for="Value"></span>
        </div>
       

        <button type="submit" class="btn btn-primary me-2">Submit</button>
        <button class="btn btn-light">Cancel</button>
    </form>
</div>
